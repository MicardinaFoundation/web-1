@model CalculatorAPI.Data.Cathegory
@{
    ViewData["Title"] = "Edit value";
}

<div class="text-start">
    <h1 class="display-4">Edit Cathegories</h1>
    <form method="post">
        <input type="hidden" name="Id" value="@Model.Id" />
        <div class="mb-3">
            <label class="form-label">Name Cathegory</label>
            <input type="text" name="Cathegories" value="@Model.Cathegories" class="form-control" />
        </div>
        <button type="submit" class="btn btn-primary">Save</button>
    </form>
</div>
